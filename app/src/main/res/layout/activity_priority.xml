<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".priority">

    <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="715dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp" />

    <TextView
        android:id="@+id/food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="15dp"
        />

    <TextView
        android:id="@+id/entertainment"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="70dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       />

    <TextView
        android:id="@+id/car"
        android:layout_width="wrap_content"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="126dp"
        android:layout_height="wrap_content"
         />

    <TextView
        android:id="@+id/home"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="170dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         />

    <TextView
        android:id="@+id/clothing"
        android:layout_width="wrap_content"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="230dp"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/electronics"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="280dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         />

    <TextView
        android:id="@+id/healthbeauty"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="330dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/children"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="400dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         />

    <TextView
        android:id="@+id/work"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="450dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/other"
        android:layout_marginHorizontal="290dp"
        android:layout_marginVertical="500dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         />
</RelativeLayout>