    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    />

<TextView
    android:id="@+id/car"
    android:layout_width="wrap_content"
    android:layout_marginHorizontal="290dp"
    android:layout_marginVertical="186dp"
    android:layout_height="wrap_content"
    />

<TextView
    android:id="@+id/home"
    android:layout_marginHorizontal="290dp"
