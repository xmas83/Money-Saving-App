    android:layout_marginTop="60dp"
    tools:layout_editor_absoluteX="8dp"
    tools:layout_editor_absoluteY="8dp" />

<TextView
    android:id="@+id/food"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
